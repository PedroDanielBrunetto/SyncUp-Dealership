generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ModeloEspecifico_Cliente {
  id     String @id @default(uuid())
  nome   String @db.VarChar(255)
  modelo String @db.VarChar(255)
  email  String
  numero String @db.VarChar(12)
}
